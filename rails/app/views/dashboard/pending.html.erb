<div class="dashboard-content">
    <h1 class="m-4 fw-bold">My Appointments</h1>
    <% if @appointment.present? %>
      <%= render "pending_appointment"%>
    <% else %>
      <h4 class="m-4">You do not have any pending appointments.</h4>
    <% end %>
</div>

<%= link_to "Show Appointment History", history_path, class: "history-pending-button rounded-0 position-relative float-end" %>

